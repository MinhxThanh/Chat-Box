{
  "manifest_version": 2,
  "name": "Chat Box â€“ Chat with all AI models",
  "version": "0.7.1",
  "description": "AI chat, advanced web search, document interaction, and more into a convenient sidebar.",
  "browser_action": {
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "commands",
    "<all_urls>"
  ],
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/watch?*"],
      "js": ["contentScript.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["selectionObserver.js"],
      "css": ["selection.css"]
    }
  ],
  "sidebar_action": {
    "default_title": "Chat Box",
    "default_panel": "sidebar.html"
  },
  "commands": {
    "toggle_sidebar": {
      "suggested_key": {
        "default": "Ctrl+E",
        "mac": "Command+E"
      },
      "description": "Toggle sidebar open/close"
    }
  },
  "web_accessible_resources": [
    "assets/action-icons/*.svg"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "thanh@onename.net",
      "strict_min_version": "109.0"
    }
  }
}


